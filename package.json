{
  "name": "helpful-react",
  "version": "0.1.0",
  "description": "A project template for building front-end applications with React, Flux, Gulp and Stylus.",
  "main": "server/main.js",
  "scripts": {
    "server": "node server/main.js",
    "test": "jest"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testFileExtensions": ["es6", "js", "jsx"],
    "moduleFileExtensions": ["js", "json", "es6", "jsx"],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-tools"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:HelpfulHuman/React-Project.git"
  },
  "keywords": [
    "react",
    "flux",
    "boilerplate",
    "helpful",
    "human"
  ],
  "author": "Helpful Human <hello@helpfulhuman.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/HelpfulHuman/React-Project/issues"
  },
  "homepage": "https://github.com/HelpfulHuman/React-Project",
  "dependencies": {
    "dotenv": "^1.1.0",
    "ejs": "^2.3.1",
    "express": "^4.12.4",
    "flux": "^2.0.3",
    "lodash": "^3.9.3",
    "portals": "^0.1.0",
    "react": "^0.13.3",
    "react-router": "^0.13.3",
    "webpack": "^1.9.10"
  },
  "devDependencies": {
    "babel": "^5.5.4",
    "babel-core": "^5.5.4",
    "babel-jest": "^5.2.0",
    "babel-loader": "^5.1.4",
    "jest-cli": "*",
    "del": "^1.2.0",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^2.3.1",
    "gulp-minify-css": "^1.1.6",
    "gulp-nodemon": "^2.0.3",
    "gulp-plumber": "^1.0.1",
    "gulp-rename": "^1.2.2",
    "gulp-stylus": "^2.0.3",
    "gulp-uglify": "^1.2.0",
    "gulp-util": "^3.0.5",
    "node-libs-browser": "^0.5.2",
    "phantomjs": "^1.9.17",
    "react-hot-loader": "^1.2.7",
    "webpack-dev-server": "^1.9.0"
  }
}
